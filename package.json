{
  "name": "job-application-tracker-v2",
  "productName": "Job Application Tracker",
  "version": "0.2.0",
  "description": "Simple job application tracker and CV generator",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "repository": "https://github.com/TimothyJNewman/job-application-tracker-v2",
  "keywords": [
    "electron",
    "sqlite3"
  ],
  "author": {
    "name": "TimothyJNewman",
    "email": "84350177+TimothyJNewman@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "job-application-tracker-v2"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "job_application_tracker"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "TimothyJNewman",
              "name": "job-application-tracker-v2"
            }
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.js",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/renderer/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.64",
    "@electron-forge/publisher-github": "^6.0.0-beta.66",
    "@types/react": "^18.0.21",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "electron": "19.0.8",
    "electron-rebuild": "^3.2.8",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.6",
    "ts-loader": "^9.4.1",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.2.6",
    "better-sqlite3": "^7.6.2",
    "dotenv": "^16.0.2",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "node-latex": "^3.1.0",
    "objects-to-csv": "^1.3.6",
    "pdf-to-base64": "^1.0.3",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.4.0",
    "react-pdf": "^5.7.2",
    "react-router-dom": "^6.3.0",
    "tw-elements": "^1.0.0-alpha12"
  },
  "engines": {
    "node": "^16"
  }
}
