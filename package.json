{
  "name": "job-application-tracker-v2",
  "productName": "job-application-tracker-v2",
  "version": "1.0.0",
  "description": "Simple job application tracker and CV generator",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-linux": "electron-forge make --platform linux --targets deb",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "postinstall": "electron-rebuild -f -w sqlite3"
  },
  "repository": "https://github.com/TimothyJNewman/job-application-tracker-v2",
  "keywords": [
    "electron",
    "sqlite3"
  ],
  "author": {
    "name": "TimothyJNewman",
    "email": "84350177+TimothyJNewman@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "job_application_tracker_v2"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.js",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/renderer/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.64",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "autoprefixer": "^10.4.7",
    "css-loader": "^6.7.1",
    "electron": "19.0.8",
    "electron-rebuild": "^3.2.8",
    "node-loader": "^2.0.0",
    "postcss-loader": "^7.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.6"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.2.6",
    "aws-sdk": "^2.1181.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.2.9",
    "node-latex": "^3.1.0",
    "pdf-to-base64": "^1.0.3",
    "prettier": "^2.7.1",
    "react-bootstrap-icons": "^1.8.4",
    "react-router-dom": "^6.3.0",
    "sqlite3": "^5.0.10"
  }
}
